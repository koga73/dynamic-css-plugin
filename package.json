{
	"name": "react-dynamic-css-plugin",
	"version": "2.0.0",
	"description": "Webpack plugin to transform dynamic react class names to short prefixed and obfuscated classes at runtime",
	"type": "module",
	"exports": {
		"./vite": {
			"import": "./src/vite/index.js"
		},
		"./webpack": {
			"import": "./src/webpack/index.js"
		},
		"./patch/react-dom": {
			"import": "./src/patch/react-dom.js"
		}
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"keywords": [
		"react",
		"dom",
		"webpack",
		"rollup",
		"vite",
		"postcss",
		"esbuild",
		"plugin",
		"css",
		"style",
		"class",
		"classname",
		"prefix",
		"obfuscate",
		"hash",
		"dynamic",
		"runtime"
	],
	"author": {
		"name": "AJ Savino"
	},
	"license": "MIT",
	"dependencies": {
		"magic-string": "^0.30.18",
		"string-replace-loader": "3.1.0"
	},
	"peerDependencies": {
		"css-loader": ">=6.9.0",
		"react-dom": "*",
		"webpack": "*",
		"vite": "*"
	},
	"peerDependenciesMeta": {
		"css-loader": {
			"optional": true
		},
		"webpack": {
			"optional": true
		},
		"vite": {
			"optional": true
		}
	}
